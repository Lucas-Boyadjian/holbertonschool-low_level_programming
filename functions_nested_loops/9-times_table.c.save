#include "main.h"
/*
* times_table - Prints the 9 times table, starting with 0.
*
*/

void times_table(void)

{
int n;
int c;


	for (n = 0; n <= 9; n++)
	{
		for (c = 0; c <= 9; c++)
		{
			_putchar(',');
			_putchar(' ');

			if (n * c <= 9)
				_putchar(' ');
			else
				_putchar((n * c / 10) + '0');

				_putchar((n * c % 10) + '0');
		}
		_putchar('\n');
	}
}
